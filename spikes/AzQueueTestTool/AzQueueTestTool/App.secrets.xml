<?xml version="1.0" encoding="utf-8" ?>
<appSettings>
	<!--Azure Storage Information-->
	<add key="storageConnectionString" value="" />

	<!--Authentication it requires Application.ReadWrite.All , User.ReadWrite.All, Directory.ReadWrite.All-->
	<add key="clientId" value="" />
	<add key="clientSecret" value="" />
	<add key="tenantId" value="" />

	<!--Service Principal Name patterns and settings-->
	<add key="servicePrincipalPrefix" value="sp" />
	<add key="servicePrincipalBaseName" value="AzQueueTesting" />
	<!--How many service principals will be created Test Case -->
	<add key="numberOfServicePrincipalObjectsToCreatePerTestCase" value="3" />
	
	<!--User Name patterns -->
	<add key="userPrefix" value="user" />
	<add key="userBaseName" value="AzQueueTesting" />
	<!--How many users will get created be Test Case -->
	<add key="numberOfUsersToCreatePerTestCase" value="3" />


	<!--Test Email pattern, email name used to created valid email other that AAD email -->
	<add key="testEmailBase" value="TestEmail"/>
	<add key="testEmailDomainNames" value="microsoft.com, outlook.com, gmail.com, amazon.com, yahoo.com" />
	<add key="includeRandomStringToTestEmail" value="false"/>
	<!--How many generated emails will be set to Notes -->
	<add key="testEmailCount" value="5"/>

	<!--What TestCase will get executed -->
	<add key="TargetTestCase" value="RuleSet1, RuleSet2, RuleSet3, RuleSet4, RuleSet5, RuleSet6, RuleSet7, RuleSet8, RuleSet9, RuleSet10, RuleSet11" />
</appSettings>


