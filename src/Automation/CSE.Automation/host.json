{
  "version": "2.0",
  "functionTimeout": "01:30:00",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:15",
      "visibilityTimeout": "00:00:30",
      "batchSize": 16,
      "maxDequeueCount": 3,
      "newBatchThreshold": 8
    }
  },
  "tracing": {
    "consoleLevel": "verbose",
    "fileLoggingMode": "debugOnly"
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Debug"
    },
    "console": {
      "isEnabled": true
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    }
  }
}